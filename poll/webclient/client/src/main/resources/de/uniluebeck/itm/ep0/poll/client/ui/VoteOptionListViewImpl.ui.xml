<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style src="PollAppUi.css"/>

    <g:DecoratorPanel>
        <g:VerticalPanel>
            <g:Label ui:field="lblOptionListName" addStyleNames="{style.optionListName}"></g:Label>
            <g:FlexTable ui:field="tblVotes" width="100%"/>
        </g:VerticalPanel>
    </g:DecoratorPanel>

</ui:UiBinder>