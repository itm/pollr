<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<module rename-to="pollapp">
    <!-- Inherit the core Web Toolkit stuff. -->
    <inherits name="com.google.gwt.user.User"/>

    <!-- Inherit the default GWT style sheet. You can change -->
    <!-- the theme of your GWT application by uncommenting -->
    <!-- any one of the following lines. -->
    <inherits name="com.google.gwt.user.theme.standard.Standard"/>

    <!-- Define custom user agent to speedup the compile process -->
    <!--set-property name="user.agent" value="safari,gecko1_8"/ -->
    <set-property name="user.agent" value="safari"/>

    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
    <!-- Other module inherits -->
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <!-- Custom module inherits -->
    <inherits name="de.uniluebeck.itm.ep0.poll.PollAppSharedXo"/>
    <inherits name="de.uniluebeck.itm.ep0.poll.PollAppApi"/>
    <inherits name="de.uniluebeck.itm.ep0.poll.PollAppShared"/>

    <!-- Specify the app entry point class. -->
    <entry-point class="de.uniluebeck.itm.ep0.poll.client.PollApp"/>

    <!-- Specify the paths for translatable code -->
    <source path="client"/>

    <!-- Specify available locales -->
    <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="de"/>

    <set-property-fallback name="locale" value="en"/>
</module>